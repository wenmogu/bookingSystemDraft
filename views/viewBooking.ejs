<%= JSON.stringify(profile) %>
<br/>
gid: <%= groupid%>
<br/>
booking: <%= booking%>
<br/>

<!DOCTYPE html>
<html lang="en">
	<head>	
		<meta charset="utf-8">
		<title>Hello, <%= profile.displayName %></title>
	</head>
	<body>
		<h1>Your Group ID: <%= groupid %></h1>
		<h1>Welcome to View Booking page, <%= profile.displayName %></h1>

		<% if (groupid == 1) { %>
			<a href="/adminBooking">View Booking By Admin</a>
			<a href='/admin'> Go To Admin Page </a>
		<% } %>


		<a href="/logout">Logout</a>

		<% for (var d=booking.length - 1; d > -1; d--) { %>
			<h3> DATE: <%=dates[d] %></h3>
			
			<% if (booking[d].length == 0) { %>
				<p>No booking slot</p>
				<br/><br/>
			<% } else { %>
				<% for (var i = 0; i < booking[d].length; i++) { %>
					<p>GROUP ID: <%=booking[d][i].groupid%></p>
					<p>ROOMNUMBER: <%=booking[d][i].roomid%></p>
					<p>TIMESLOT STARTS AT: <%=booking[d][i].start%></p>
					<p>TIMESLOT ENDS AT: <%=booking[d][i].end%></p>
					
					<% if (dateAndTimeString.dateString == dates[d]) { %>
						<% if (dateAndTimeString.timeString > booking[d][i].start) { %>

							<% if (groupid == 1) { %>
								<!-- admin button -->
								<p><button onclick="location.href ='/adminBookRecord'"> Go To Admin Booking Info Page </button></p>
							<% } else { %>
								<!-- normal user cancel button -->
								<p><button onclick="location.href ='/cancelBooking?room=<%=booking[d][i].roomid%>&start=<%=booking[d][i].start%>&end=<%=booking[d][i].end%>&date=<%=dates[d] %>'" disabled> Cancel Booking </button></p>
							<% } %>
							
						<% } else { %>

							<% if (groupid == 1) { %>
								<!-- admin button -->
								<p><button onclick="location.href ='/adminBookRecord'"> Go To Admin Booking Info Page </button></p>
							<% } else { %>
								<!-- normal user cancel button -->
								<p><button onclick="location.href ='/cancelBooking?room=<%=booking[d][i].roomid%>&start=<%=booking[d][i].start%>&end=<%=booking[d][i].end%>&date=<%=dates[d] %>'"> Cancel Booking </button></p>
							<% } %>
							
						<% } %>
					<% } else { %>

						<% if (groupid == 1) { %>
							<!-- admin button -->
							<p><button onclick="location.href ='/adminBookRecord'"> Go To Admin Booking Info Page </button></p>
						<% } else { %>
							<!-- normal user cancel button -->
							<p><button onclick="location.href ='/cancelBooking?room=<%=booking[d][i].roomid%>&start=<%=booking[d][i].start%>&end=<%=booking[d][i].end%>&date=<%=dates[d] %>'"> Cancel Booking </button></p>
						<% } %>
						
					<% } %>			
					<br/><br/>
				<% } %>
			<% } %>
		<% } %>

		<a href="/info">View General Info</a>
	</body>
</html>