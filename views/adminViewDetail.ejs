  	<div class="container">
  		<div class="row">
  			<div class="col-lg-7 col-center-block">
  				
  				<h4>Booking info: </h4>

  				<form action='/adminDeleteBookingInfo' method="post" class="form-horizontal">
  					<fieldset>

  						<% if (info[0] == undefined) {%>

  						<table class="table table-striped table-hover ">
  							<tbody>
  								<tr>
  									<td>Group ID: </td>
  									<td><input type="text" name='gid' value='<%= _info[0].groupid %>' hidden><%= _info[0].groupid %></td>
  								</tr>

  								<tr>
  									<td>Room Number: </td>
  									<td><input type="text" name='rid' value='<%= _info[0].roomid %>' hidden><%= _info[0].roomid %></td>
  								</tr>

  								<tr>
  									<td>Date: </td>
  									<td><input type="text" name='d' value='<%= _info[0].date %>' hidden><%= _info[0].date %>
  									</tr>

  									<tr>
  										<td>Time start: </td>
  										<td><input type="text" name='start' value='<%= _info[0].start %>' hidden><%= _info[0].start %></td>
  									</tr>

  									<tr>
  										<td>Time end: </td>
  										<td><input type="text" name='end' value='<%= _info[0].end %>' hidden><%= _info[0].end %></td>
  									</tr>



  								</tbody>
  							</table>

  							<% } else {%>

  							<table class="table table-striped table-hover ">
  								<tbody>
  									<tr>
  										<td>Group ID: </td>
  										<td><input type="text" name='gid' value='<%= info[0].groupid %>' hidden><%= info[0].groupid %></td>
  									</tr>

  									<tr>
  										<td>Room Number: </td>
  										<td><input type="text" name='rid' value='<%= info[0].roomid %>' hidden><%= info[0].roomid %></td>
  									</tr>

  									<tr>
  										<td>Date: </td>
  										<td><input type="text" name='d' value='<%= info[0].date %>' hidden><%= info[0].date %>
  										</tr>

  										<tr>
  											<td>Time start: </td>
  											<td><input type="text" name='start' value='<%= info[0].start %>' hidden><%= info[0].start %></td>
  										</tr>

  										<tr>
  											<td>Time end: </td>
  											<td><input type="text" name='end' value='<%= info[0].end %>' hidden><%= info[0].end %></td>
  										</tr>



  									</tbody>
  								</table>
  								<% } %>


  								<div class="form-group">
  									<div class="col-lg-10 col-lg-offset-2">
  										<button type="submit" class="btn btn-danger">Delete The Above Record</button>
  									</div>
  								</div>

  							</fieldset>
  						</form>
  					</div>
  				</div>
