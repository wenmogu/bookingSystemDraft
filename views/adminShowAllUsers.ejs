<html>
  <head>
    <title>Hello, <%= profile.displayName %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Hello, <%= profile.displayName %></h1>
    <h3>Group <%= profile.groupid %></h3>
    <a href="/admin">Go To Admin Page</a>


	<table>
		<tr>
			<th>Group ID <th>
			<th>User Name <th>
			<th>User NUSNETSID <th>
			<th>User Email <th>
		</tr>
		<% for (var i = 0; i < info.length; i++) { %>
			<tr>
				<th><%= info[i].groupid %></th>
				<th><%= info[i].name %></th>
				<th><%= info[i].uid %></th>
				<th><%= info[i].email %></th>
			</tr>
		<% } %>
	</table>

  </body>
</html>